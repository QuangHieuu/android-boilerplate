<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <size android:width="@dimen/dp_36" android:height="@dimen/dp_20" />
            <solid android:width="@dimen/dp_1" android:color="@color/colorPrimary" />
            <corners android:radius="@dimen/dp_50" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <size android:width="@dimen/dp_36" android:height="@dimen/dp_20" />
            <solid android:width="@dimen/dp_1" android:color="@color/color_D9D9D9" />
            <corners android:radius="@dimen/dp_50" />
        </shape>
    </item>
</selector>